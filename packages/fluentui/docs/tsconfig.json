{
  "extends": "../../../scripts/typescript/tsconfig.fluentui",
  "compilerOptions": {
    "module": "esnext",
    "paths": {
      "@fluentui/react": ["packages/react/src"],
      "@fluentui/react/lib/*": ["packages/react/src/*"],
      "@fluentui/*": ["packages/fluentui/*/src", "packages/*/src"],
      "@uifabric/*": ["packages/*/src"],
      "src/*": ["packages/fluentui/react-northstar/src/*"],
      "test/*": ["packages/fluentui/react-northstar/test/*"]
    },
    "typeRoots": ["../../../typings", "../../../node_modules/@types"],
    "types": ["node", "webpack-env", "screener-runner"]
  },
  "include": ["./src"]
}
